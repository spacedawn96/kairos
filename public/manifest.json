{
  "manifest_version": 2,
  "name": "kairos",
  "short_name": "kairos",
  "description": "Browser extension",
  "version": "1.1.1",
  "icons": {
  },
  "background": {
    "page": "index.html"
  },
  "browser_action": {
    "default_icon": "logo192.png",
    "default_title": "View web browsing activity"
  },
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "permissions": [
    "idle",
    "tabs"
  ]
}
